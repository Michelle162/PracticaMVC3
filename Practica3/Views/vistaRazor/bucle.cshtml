@{
    ViewBag.Title = "Bucle";
    var cantidadLineas = 10;
}
<h2>
    bucles
</h2>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < cantidadLineas; i++)
        {
            <tr>
                <td>@i</td>
                <td>Nombre No.</td>
            </tr>

        }
    </tbody>



</table>
